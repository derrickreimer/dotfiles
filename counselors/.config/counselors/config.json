{
  "version": 1,
  "defaults": {
    "timeout": 540,
    "outputDir": "./agents/counselors",
    "readOnly": "bestEffort",
    "maxContextKb": 50,
    "maxParallel": 4
  },
  "tools": {
    "claude-opus": {
      "binary": "/Users/derrickreimer/.local/bin/claude",
      "readOnly": {
        "level": "enforced"
      },
      "adapter": "claude",
      "extraFlags": [
        "--model",
        "opus"
      ]
    },
    "codex-5.3-high": {
      "binary": "/Users/derrickreimer/.local/share/mise/installs/node/18.20.8/bin/codex",
      "readOnly": {
        "level": "enforced"
      },
      "timeout": 900,
      "adapter": "codex",
      "extraFlags": [
        "-m",
        "gpt-5.3-codex",
        "-c",
        "model_reasoning_effort=high"
      ]
    },
    "codex-5.3-xhigh": {
      "binary": "/Users/derrickreimer/.local/share/mise/installs/node/18.20.8/bin/codex",
      "readOnly": {
        "level": "enforced"
      },
      "timeout": 900,
      "adapter": "codex",
      "extraFlags": [
        "-m",
        "gpt-5.3-codex",
        "-c",
        "model_reasoning_effort=xhigh"
      ]
    },
    "gemini-3.1-pro": {
      "binary": "/opt/homebrew/bin/gemini",
      "readOnly": {
        "level": "enforced"
      },
      "timeout": 900,
      "adapter": "gemini",
      "extraFlags": [
        "-m",
        "gemini-3.1-pro-preview"
      ]
    }
  },
  "groups": {
    "smart": ["claude-opus", "codex-5.3-xhigh", "gemini-3.1-pro"]
  }
}
